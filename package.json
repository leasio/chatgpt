  "scripts": {
    "start": "npm-run-all --parallel \"dev\" \"dev:client\" \"dev:server\"",
    "dev": "nodemon dist/server-entry.js",
    "dev:client": "cross-env NODE_ENV=development webpack --config config/webpack.client.js --watch",
    "dev:server": "cross-env NODE_ENV=development webpack --config config/webpack.server.js --watch",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.js"
  },
